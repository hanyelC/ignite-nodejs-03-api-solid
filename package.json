{
  "name": "03-api-solid",
  "version": "1.0.0",
  "description": "GymPass style app.",
  "main": "index.js",
  "scripts": {
    "start": "node build/server.js",
    "start:dev": "tsx watch src/server.ts",
    "services:up": "docker compose up -d",
    "services:stop": "docker compose stop",
    "build": "tsup src --out-dir build",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanyelC/ignite-nodejs-03-api-solid.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hanyelC/ignite-nodejs-03-api-solid/issues"
  },
  "homepage": "https://github.com/hanyelC/ignite-nodejs-03-api-solid#readme",
  "dependencies": {
    "@fastify/jwt": "6.7.0",
    "@prisma/client": "4.11.0",
    "bcrypt": "5.1.0",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "fastify": "4.14.1",
    "tsup": "6.6.3",
    "tsx": "3.12.3",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/node": "18.14.6",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "@vitest/coverage-c8": "0.29.2",
    "@vitest/ui": "0.29.2",
    "eslint": "8.35.0",
    "eslint-plugin-align-import": "1.0.0",
    "npm-run-all": "4.1.5",
    "prisma": "4.11.0",
    "typescript": "4.9.5",
    "vite-tsconfig-paths": "4.0.5",
    "vitest": "0.29.2"
  }
}
